{
  "extends": "@react-native/typescript-config/tsconfig.json",
  "compilerOptions": {
    "types": [
      "jest",
      "node"
    ],
    "typeRoots": [
      "@types",
      "./node_modules/@types"
    ],
    "baseUrl": "./",
    "paths": {
      "@/*": [
        "./src/*"
      ],
      "types/*": [
        "./@types/*"
      ]
    },
    "allowImportingTsExtensions": true
  }
}
